generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String

  Sessions Sessions[]
}

model Sessions {
  id String @id @unique
  expires_at Int
  user_id Int
  
  user User @relation(fields: [user_id], references: [id])
}

model Trainings {
  id Int @id @default(autoincrement())
  title String
  image String
  description String
}
